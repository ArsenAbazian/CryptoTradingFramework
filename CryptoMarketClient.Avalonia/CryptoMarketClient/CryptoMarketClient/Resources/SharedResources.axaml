<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mxe="https://schemas.eremexcontrols.net/avalonia/editors"
                    xmlns:controls="clr-namespace:CryptoMarketClient.Controls">
    <DataTemplate x:Key="UltraCompactBuySellCellTemplate">
        <controls:BuySellCellControl Classes="UltraCompact" Text="{Binding Value}" OrderType="{Binding Row.Type}"/>
    </DataTemplate>
    <DataTemplate x:Key="UltraCompactCellTemplate">
        <mxe:TextEditor Classes="UltraCompact" x:Name="PART_Editor"/>
    </DataTemplate>
    <DataTemplate x:Key="UltraCompactBidCellTemplate">
        <mxe:TextEditor Classes="UltraCompact" x:Name="PART_Editor" Foreground="{StaticResource BidColor}"/>
    </DataTemplate>
    <DataTemplate x:Key="UltraCompactAskCellTemplate">
        <mxe:TextEditor Classes="UltraCompact" x:Name="PART_Editor" Foreground="{StaticResource AskColor}"/>
    </DataTemplate>
    
    <DataTemplate x:Key="UltraCompactTimeCellTemplate">
        <mxe:TextEditor Classes="UltraCompact" x:Name="PART_Editor" DisplayFormatString="hh:mm:ss.fff"/>
    </DataTemplate>
</ResourceDictionary>
